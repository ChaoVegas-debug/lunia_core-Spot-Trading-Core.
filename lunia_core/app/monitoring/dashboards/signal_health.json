{
  "title": "Signal Health",
  "uid": "signal-health",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "30s",
  "tags": ["lunia", "signal"],
  "panels": [
    {
      "id": 1,
      "title": "Signal Accuracy",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(lunia_signals_total[5m]) > 0)",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "title": "LLM Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le)(rate(lunia_llm_latency_ms_bucket[5m])))",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "title": "AI Research Signals",
      "type": "bargauge",
      "targets": [
        {
          "expr": "sum by (strategy)(increase(lunia_ai_research_signal_total[1h]))",
          "refId": "A"
        }
      ]
    }
  ]
}
